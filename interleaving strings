class Solution(object):
    def isInterleave(self, s1, s2, s3):
        if len(s1) + len(s2) != len(s3): 
            return False

        dp = [ [False] * (len(s2) + 1) for i in range(len(s1) + 1)] #creates the grid and fills it with false values
        dp[len(s1)][len(s2)] = True #out of bound corner value as true 

        for i in range(len(s1), -1, -1):
            for j in range(len(s2), -1, -1): #starts loop bottom right and goes backward through the grid row by row
                if i < len(s1) and s1[i] == s3[i+j] and dp[i+1][j]:
                    dp[i][j] = True 
                if j < len(s2) and s2[j] == s3[i + j] and dp[i][j+1]: 
                    dp[i][j] = True 
        return dp[0][0]
        
        dp = {} 
        def dfs(i , j): 
            if i == len(s1) and j == len(s2): #if i and j are out of bounds, return true
                return True 
            if (i,j) in dp: #if the position is computed, its already in the cache.
                return dp[(i,j)] #return result 
            if i < len(s1) and s1[i] == s3[i +j] and dfs(i +1, j): 
                return True 
            if j < len(s2) and s2[j] == s3[i +j] and dfs(i, j + 1):
                return True 
            dp[(i,j)] = False #if neither return true, we cache as false
            return False #return False to not repeat the work again. 
